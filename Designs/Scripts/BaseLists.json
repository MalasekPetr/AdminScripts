                },
                {
                    "verb": "addSPView",
                    "name": "All Documents",
                    "viewFields": [
                        "DocIcon",
                        "LinkFilename",
                        "Modified",
                        "Editor"
                    ],
                    "query": "<OrderBy><FieldRef Name=\"FileLeafRef\" /></OrderBy>",
                    "rowLimit": 30,
                    "isPaged": true,
                    "makeDefault": true
                }
            ]
        },
        {
            "verb": "createSPList",
            "listName": "DocExtends",
            "hidden": false,
            "templateType": 100,
            "subactions": [
                {
                    "verb": "addContentType",
                    "name": "DocExtend"
                },
                {
                    "verb": "removeContentType",
                    "name": "Item"
                }
            ]
        }
    ],
    "bindata": { },
